<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="Evento.model.Picture" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table ="Picture">
      <id column="Id_Picture" name="Id_Picture" type = "long"><generator class="increment"/></id>
      <property column="Name" name ="Name" type = "string" not-null="true"></property>
      <property column="CreatedAt" name ="CreatedAt" type = "string"></property>
      <property column="Link" name ="Link" type = "string"></property>
      <property column="TymczasowyBezposredniLink" name ="TymczasowyBezposredniLink" type = "string"></property>
      <many-to-one name="Id_User" column="Id_User" class="Evento.model.User" not-null="true" foreign-key="fk_picture_user" cascade="delete"></many-to-one>
      <many-to-one name="Id_Event" column="Id_Event" class="Evento.model.Event" not-null="false" foreign-key="fk_picture_event" cascade="delete"></many-to-one>
      <many-to-one name="Id_Album" column="Id_Album" class="Evento.model.Album" foreign-key="fk_picture_album" cascade="delete"></many-to-one>
  </class>
</hibernate-mapping>